#include <stdio.h>
#include <stdlib.h>

int main()
{
    int i, j;
    int num;


    do
    {
        printf("\n entre com um numero positivo:");
        scanf("%d", &num);
    }
    while( num <= 0);


    for(i==1 ; i<=num ; i++)
    {
         printf("\nesta linha eh a linha %d", i);
    }
      printf("\n");

  system("pause");
    return 0;
}
